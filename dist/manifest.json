{
  "manifest_version": 3,
  "name": "Hitokoto",
  "version": "0.0.5",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "permissions": [
    "bookmarks"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  }
}